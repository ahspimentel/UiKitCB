{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ui",
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "assets": [
    {
      "glob": "**/*",
      "input": "src/assets/",
      "output": "/assets"
    },
    {
      "glob": "**/*",
      "input": "src/theme/",
      "output": "/theme"
    }
]
}